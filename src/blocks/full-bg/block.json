{
    "apiVersion": 3,
    "name": "framos/full-bg",
    "version": "1.0.0",
    "title": "Full BG",
    "category": "framos-blocks",
    "description": "A full-width background block with a heading, text, and two buttons.",
    "supports": {
        "html": false,
        "anchor": true,
        "align": ["wide", "full"]
    },
    "example": {
        "attributes": {
            "preview": true
        },
        "viewportWidth": 432
    },
    "attributes": {
        "containerClasses": {
            "type": "array",
            "default": []
        },
        "containerBgImage": {
            "type": "object"
        },
        "heading": {
            "type": "string",
            "default": "Data to enrich your online business"
        },
        "headingLevel": {
            "type": "string",
            "default": "h2"
        },
        "headingClasses": {
            "type": "array",
            "default": []
        },
        "subHeading": {
            "type": "string",
            "default": "Announcing our next round of funding"
        },
        "subHeadingClasses": {
            "type": "array",
            "default": []
        },
        "text": {
            "type": "string",
            "default": "Anim aute id magna aliqua ad ad non deserunt sunt. Qui irure qui lorem cupidatat commodo. Elit sunt amet fugiat veniam occaecat."
        },
        "textClasses": {
            "type": "array",
            "default": []
        },
        "primaryButtonText": {
            "type": "string",
            "default": "Get started"
        },
        "primaryButtonUrl": {
            "type": "object",
            "default": {
                "url": "#",
                "isExternal": false
            }
        },
        "primaryButtonClasses": {
            "type": "array",
            "default": []
        },
        "secondaryButtonText": {
            "type": "string",
            "default": "Learn more"
        },
        "secondaryButtonUrl": {
            "type": "object",
            "default": {
                "url": "#",
                "isExternal": false
            }
        },
        "secondaryButtonClasses": {
            "type": "array",
            "default": []
        }
    },
    "textdomain": "framos-custom-blocks",
    "editorScript": "file:./index.js"
}
