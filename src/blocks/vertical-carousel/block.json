{
    "apiVersion": 3,
    "name": "framos/vertical-carousel",
    "version": "1.0.0",
    "title": "Vertical Carousel",
    "category": "framos-blocks",
    "description": "A custom block for a vertical carousel.",
    "supports": {
        "html": false,
        "anchor": true,
        "align": ["wide", "full"]
    },
    "example": {
        "attributes": {
            "preview": true
        },
        "viewportWidth": 432
    },
    "attributes": {
        "containerClasses": {
            "type": "array",
            "default": []
        },
        "heading": {
            "type": "string",
            "default": "Effortless Communication at Your Fingertips"
        },
        "headingLevel": {
            "type": "string",
            "default": "h2"
        },
        "headingClasses": {
            "type": "array",
            "default": []
        },
        "text": {
            "type": "string",
            "default": "Experience effortless communication with our secure and intuitive chat application. Stay connected with friends, family, and colleagues wherever you are."
        },
        "textClasses": {
            "type": "array",
            "default": []
        },
        "primaryButtonText": {
            "type": "string",
            "default": "Install Now"
        },
        "primaryButtonUrl": {
            "type": "object",
            "default": {
                "url": "#",
                "isExternal": false
            }
        },
        "primaryButtonClasses": {
            "type": "array",
            "default": []
        },
        "pbTextClasses": {
            "type": "array",
            "default": []
        },
        "secondaryButtonText": {
            "type": "string",
            "default": "Learn more"
        },
        "secondaryButtonUrl": {
            "type": "object",
            "default": {
                "url": "#",
                "isExternal": false
            }
        },
        "secondaryButtonClasses": {
            "type": "array",
            "default": []
        },
        "sbTextClasses": {
            "type": "array",
            "default": []
        },
        "sliderWrapperClasses": {
            "type": "array",
            "default": []
        },
        "sliderOneImages": {
            "type": "array",
            "default": []
        },
        "sliderOneClasses": {
            "type": "array",
            "default": []
        },
        "sliderTwoImages": {
            "type": "array",
            "default": []
        },
        "sliderTwoClasses": {
            "type": "array",
            "default": []
        }
    },
    "textdomain": "framos-custom-blocks",
    "editorScript": ["file:./index.js", "framos-swiper"],
    "style": ["file:./style-index.css", "framos-swiper"],
    "viewScript": ["file:./view.js", "framos-swiper"]
}
