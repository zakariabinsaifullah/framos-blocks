        },
        "headingLevel": {
            "type": "string",
            "default": "h2"
        },
        "headingClasses": {
            "type": "array",
            "default": []
        },
        "title": {
            "type": "string",
            "default": "Lorem ipsum dolor sit amet "
        },
        "titleClasses": {
            "type": "array",
            "default": []
        }
    },
    "textdomain": "framos-custom-blocks",
    "editorScript": "file:./index.js"
}
