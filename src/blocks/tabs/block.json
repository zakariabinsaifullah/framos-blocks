            "type": "string",
            "default": "We are here to help you grow your business. Our team of experts is ready to assist you with any questions or concerns you may have."
        },
        "tiDescClasses": {
            "type": "array",
            "default": []
        },
        "tiButtonText": {
            "type": "string",
            "default": "Get Started"
        },
        "tiButtonLink": {
            "type": "object",
            "default": {
                "url": "#",
                "isExternal": false
            }
        },
        "tiBtnClasses": {
            "type": "array",
            "default": []
        },
        "cardClasses": {
            "type": "array",
            "default": []
        }
    },
    "textdomain": "framos-custom-blocks",
    "editorScript": "file:./index.js",
    "viewScript": "file:./view.js"
