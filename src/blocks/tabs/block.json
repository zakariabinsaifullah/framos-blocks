    "attributes": {
        "tabTitles": {
            "type": "array",
            "default": []
        },
        "tabChildCount": {
            "type": "number",
            "default": 4
        },
        "tabTitleClasses": {
            "type": "array",
            "default": []
        },
        "tiTitle": {
            "type": "string",
            "default": "Grow faster with proâ€™s help"
        },
        "tiTitleClasses": {
            "type": "array",
            "default": []
