{
    "apiVersion": 3,
    "name": "framos/content-image",
    "version": "1.0.0",
    "title": "Content Image",
    "category": "framos-blocks",
    "description": "Display content and image side by side.",
    "supports": {
        "html": false,
        "anchor": true,
        "align": ["wide", "full"]
    },
    "example": {
        "attributes": {
            "preview": true
        },
        "viewportWidth": 432
    },
    "attributes": {
        "containerClasses": {
            "type": "array",
            "default": []
        },
        "heading": {
            "type": "string",
            "default": "Why Choose FRAMOS for Global Shutter Image Sensors Solutions?"
        },
        "headingLevel": {
            "type": "string",
            "default": "h2"
        },
        "headingClasses": {
            "type": "array",
            "default": []
        },
        "text": {
            "type": "string",
            "default": "Incididunt sint fugiat pariatur cupidatat consectetur sit cillum anim id veniam aliqua proident excepteur commodo do ea."
        },
        "textClasses": {
            "type": "array",
            "default": []
        },
        "image": {
            "type": "object"
        },
        "layoutClasses": {
            "type": "array",
            "default": []
        },
        "contentSideClasses": {
            "type": "array",
            "default": []
        },
        "imageSideClasses": {
            "type": "array",
            "default": []
        },
        "changeOrder": {
            "type": "boolean",
            "default": false
        }
    },
    "textdomain": "framos-custom-blocks",
    "editorScript": "file:./index.js",
    "viewScript": "file:./view.js",
    "render": "file:./render.php"
}
